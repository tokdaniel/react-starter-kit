{
  "name": "react-starter-project",
  "version": "0.0.1",
  "description": "starter project for react applications with webpack",
  "main": "src/app/app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack-dev-server --progress --colors --config webpack/dev.js",
    "clean": "rimraf build",
    "build": "rimraf build/prod && cross-env NODE_ENV=prod webpack --progress --colors --output-path build/prod --config webpack/prod.js",
    "build-dev": "rimraf build/dev && cross-env NODE_ENV=dev webpack --progress --colors --output-path build/dev --config webpack/dev.js",
    "test": "jest --colors",
    "test:watch": "jest --watch --colors",
    "test:coverage": "jest --coverage --colors",
    "lint": "eslint . --color",
    "prepush": "npm run lint && npm run test"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "selectors.js",
      "actions.js"
    ],
    "modulePaths": [
      "src"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^actions(.*)$": "<rootDir>/src/app/actions$1",
      "^assets$": "<rootDir>/assets",
      "^components$": "<rootDir>/src/app/components",
      "^config$": "<rootDir>/src/app/config",
      "^containers$": "<rootDir>/src/app/containers",
      "^selectors(.*)$": "<rootDir>/src/app/selectors$1",
      "^utils(.*)$": "<rootDir>/src/app/utils"
    }
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "starter-kit"
  ],
  "author": "Daniel Tok",
  "license": "MIT",
  "dependencies": {
    "axios": "0.16.2",
    "history": "4.7.2",
    "lodash": "4.17.4",
    "material-ui": "1.0.0-beta.16",
    "material-ui-icons": "1.0.0-beta.17",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.5",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "recompose": "0.25.1",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "compression-webpack-plugin": "1.0.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.4",
    "css-loader": "0.28.4",
    "enzyme": "2.9.1",
    "eslint": "4.7.2",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "21.1.0",
    "node-sass": "4.5.3",
    "path": "0.12.7",
    "raw-loader": "0.5.1",
    "react-test-renderer": "15.6.1",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.3.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.4.1",
    "webpack-dev-server": "2.6.1",
    "webpack-notifier": "1.5.0"
  }
}
